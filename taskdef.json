{
    "family": "rails-jenkins",
    "containerDefinitions": [
        {
            "image": "%REPOSITORY_URI%:v_%BUILD_NUMBER%",
            "name": "rails-jenkins",
            "cpu": 10,
            "memory": 200,
            "essential": true,
            "portMappings": [
                {
                    "containerPort": 3000,
                    "hostPort": 0
                }
            ]
        }
    ]
}
